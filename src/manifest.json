{
    "manifest_version": 3,
    "name": "Super Chat Currency Converter",
    "short_name": "SC3",
    "version": "0.1.0",
    "homepage_url": "https://github.com/haseHH/super-chat-currency-converter#readme",
    "description": "Converts YouTube Super Chat donation amounts to your currency",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        },
        "default_popup": "popup.html",
        "default_title": "Super Chat Currency Converter"
    },
    "options_page": "popup.html",
    "background": {
        "service_worker": "service-worker.js"
    }
}
